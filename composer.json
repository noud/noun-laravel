{
    "name": "noud/nouns-laravel",
    "require": {
        "php": ">=7.2.5",
        "doctrine/inflector": "^1"
    },
    "description": "Nouns Laravel package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Noud de Brouwer",
            "email": "noud4@home.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nouns\\": "src/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nouns\\Providers\\NounsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../inflector"
        }
    ]
}